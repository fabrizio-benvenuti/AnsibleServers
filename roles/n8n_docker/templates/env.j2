DOMAIN_NAME={{ domain_name }}
SUBDOMAIN={{ subdomain }}
GENERIC_TIMEZONE={{ generic_timezone }}
SSL_EMAIL={{ ssl_email }}
# Comma-separated list of trusted proxies for Express / n8n.
# Defaults to loopback and common local addresses so reverse proxies on the host are trusted.
# Example: loopback,linklocal,uniquelocal,127.0.0.1,::1
N8N_TRUSTED_PROXIES=loopback,linklocal,uniquelocal,127.0.0.1,::1
